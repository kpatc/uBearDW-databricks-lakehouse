{
  "name": "ubear_bronze_cdc_streaming",
  "storage": "/mnt/datalake/ubear/dlt/bronze",
  "target": "ubear_bronze",
  "configuration": {
    "kafka.bootstrap.servers": "pkc-z1o60.europe-west1.gcp.confluent.cloud:9092",
    "pipelines.enableTrackHistory": "true"
  },
  "clusters": [
    {
      "label": "default",
      "spark_conf": {
        "spark.databricks.delta.optimizeWrite.enabled": "true",
        "spark.databricks.delta.autoCompact.enabled": "true"
      }
    }
  ],
  "notebooks": [
    {
      "path": "/Repos/uBearDW-databricks-lakehouse/pipelines/bronze_pipeline"
    }
  ],
  "continuous": true,
  "development": false,
  "channel": "CURRENT",
  "edition": "CORE",
  "serverless": true
}
